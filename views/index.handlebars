<h1>Wish Burger</h1>

<h3>Save Burger</h3>

<ul>
  {{#each burger}}
  	<li>
  	   <p>ID: {{this.id}}</p>
       <p>Wish: {{this.wish}}</p>
  	</li>
  {{/each}}
</ul>

<div>
  <form action="/" method="POST">
  	<textarea type="text" name="burger"></textarea>

  	<button type="submit">Enter</button>
  </form>
</div>
